{
  "schemaVersion": 1,
  "id": "mineboxtools",
  "version": "${version}",
  "name": "MineboxTools",
  "description": "A set of tools for Minebox. For any issue, contact me on discord _silv",
  "authors": [
    "Silverhn"
  ],
  "contact": {
    "sources": "https://github.com/Silv-04/MineboxTools/"
  },
  "license": "GPL-3.0",
  "icon": "icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "fr.silv.MineboxTools"
    ],
    "client": [
      "fr.silv.MineboxToolsClient"
    ]
  },
  "mixins": [
    "mineboxtools.mixins.json",
    {
      "config": "mineboxtools.client.mixins.json",
      "environment": "client"
    }
  ],
  "depends": {
    "fabricloader": ">=0.16.14",
    "minecraft": "~1.21.7",
    "java": ">=21",
    "fabric-api": "*"
  },
  "suggests": {
    "another-mod": "*"
  }
}